  "scripts": {
    "clear": "rm -rf ./node_modules/",
    "clear:build": "rm -rf ./.build",
    "clear:cache": "rm -rf ./node_modules/.cache",
    "client:build": "webpack --color --progress -p --config ./webpack/webpack.config.js",
    "client:dev": "yarn run clear:cache && webpack-dev-server --color --progress -w --config ./webpack/webpack.config.js",
    "api:start": "ts-node --typeCheck --project ./tsconfig.api.json ./api/api.ts",
    "api:build": "tsc --build ./tsconfig.api.json ./api/api.ts",
    "api:dev": "ts-node-dev -r tsconfig-paths/register --respawn --project ./tsconfig.api.json ./api/api.ts --watch ./api --typeCheck",
    "build": "yarn run webserver:build && yarn run client:build",
    "start": "node ./.build/webserver/server.js"
  },
